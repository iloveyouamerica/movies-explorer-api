{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "linebreak-style": "off"
  },
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}

